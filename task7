#include <vector>
#include <cmath> 
#include <iostream>
using namespace std;
int x_beraber_y_kvadrati(const vector<int>& x, const vector<int>& y) {
    if (x.size() != y.size()) {
        cout<< "Massivlər eyni uzunluqda olmalıdır." << endl;
        return -1;
    }
    for (size_t i = 0; i < x.size(); ++i) {
        if (x[i] == y[i] * y[i]) {
            return x[i];
        }
    }

    return -1;
}
